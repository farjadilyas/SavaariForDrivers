<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="?attr/backgroundColor"
    android:enterFadeDuration="1400"
    android:exitFadeDuration="1500">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="2dp" android:color="?attr/backgroundColor" />
            <corners android:radius="25dp" />
            <solid android:color="?attr/deselectedForegroundColor"/>
        </shape>
    </item>
</ripple>